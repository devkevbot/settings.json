{
  "[elixir]": {
    "editor.formatOnSave": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.trimAutoWhitespace": false,
    "editor.wordBasedSuggestions": false,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true
  },
  "[html][javascript][typescript][javascriptreact][typescriptreact][css][postcss][vue]": {
    "editor.codeActionsOnSave": [
      "source.fixAll.eslint"
    ],
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "[json][jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "[lua]": {
    "editor.formatOnSave": true,
    "editor.tabSize": 4
  },
  "[ocaml]": {
    "editor.formatOnSave": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.formatOnSave": true
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
    "editor.formatOnSave": true
  },
  "breadcrumbs.enabled": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.detectIndentation": false,
  "editor.fontFamily": "'JetBrains Mono','Courier New', monospace",
  "editor.fontLigatures": false,
  "editor.fontSize": 18,
  "editor.inlayHints.enabled": "offUnlessPressed",
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.semanticHighlighting.enabled": true,
  "editor.smoothScrolling": false,
  "editor.stickyScroll.enabled": true,
  "editor.tabSize": 4,
  "eslint.rules.customizations": [
    {
      "rule": "*",
      "severity": "warn"
    }
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "files.exclude": {
    "**/.DS_Store": false,
    "**/.git": false,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/Thumbs.db": true
  },
  "git.confirmSync": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.hovers.avatars": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.keymap": "alternate",
  "grammarly.config.documentDialect": "canadian",
  "grammarly.config.suggestions.OxfordComma": true,
  "grammarly.config.suggestions.PassiveVoice": true,
  "grammarly.files.include": [
    "**/readme.md",
    "**/README.md",
    "**/*.txt",
    "**/*.mdx"
  ],
  "ocaml.useOcamlEnv": false,
  "python.formatting.provider": "black",
  "rewrap.autoWrap.enabled": false,
  "rewrap.wrappingColumn": 100,
  "rust-analyzer.check.command": "clippy",
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.fontFamily": "'JetBrains Mono','Courier New', monospace",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.shellIntegration.enabled": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "window.title": "${dirty}${activeEditorShort}${separator}${rootPath}${separator}",
  "workbench.activityBar.visible": true,
  "workbench.colorCustomizations": {
    "editorLineNumber.activeForeground": "#FFF",
    "editorLineNumber.foreground": "#777"
  },
  "workbench.colorTheme": "Ros√© Pine (no italics)",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.layoutControl.enabled": true,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.statusBar.visible": true,
  "workbench.tips.enabled": false
}